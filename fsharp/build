#!/bin/sh
# the F# is automatically generated from the OCaml source
set -e
cd ConfSolveSemantics/ConfSolveSemantics
../../ocaml-to-fsharp
fsyacc Parser.fsy --module Parser
fslex Lexer.fsl --unicode
fsc2 --nologo ConfSolve.fs Semantics.fs Parser.fs Lexer.fs Program.fs -r FSharp.PowerPack.dll -o bin/Debug/ConfSolveSemantics.exe